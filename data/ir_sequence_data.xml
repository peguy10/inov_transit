<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

          <record id="sequence_transit_invoice_id" model="ir.sequence">
            <field name="name">Gestion Transit sequence</field>
            <field name="code">transit.invoice</field>
            <field name="prefix">LMC%(range_year)s%(month)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">3</field>
        </record>
        <record id="sequence_acconage_invoice_id" model="ir.sequence">
            <field name="name">Gestion Acconage sequence</field>
            <field name="code">transit.acconage</field>
            <field name="prefix">ACC%(range_year)s%(month)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">3</field>
        </record>
        <record id="sequence_shipping_invoice_id" model="ir.sequence">
            <field name="name">Gestion Shipping sequence</field>
            <field name="code">transit.shipping</field>
            <field name="prefix">SHIP%(range_year)s%(month)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">3</field>
        </record>

